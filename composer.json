{
  "name": "evrinoma/utils-bundle",
  "description": "Utils pack",
  "keywords": [
    "Utils",
    "Core"
  ],
  "type": "symfony-bundle",
  "license": "PROPRIETARY",
  "authors": [
    {
      "name": "Nikolay Nikolaev",
      "email": "evrinoma@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "doctrine/annotations": "^2.0",
    "symfony/serializer": "^6.0",
    "lexik/jwt-authentication-bundle": "^2",
    "evrinoma/dto-bundle": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Evrinoma\\UtilsBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4|^3.8",
    "symfony/dotenv": "5.2.*",
    "evrinoma/hooks-bundle": "dev-master"
  },
  "scripts": {
    "auto-scripts": {
    },
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "pre-package-uninstall": [
    ]
  },
  "repositories": {
    "hooks": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/HooksBundle.git"
    },
    "dto": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/DtoBundle.git"
    },
    "utils": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/UtilsBundle.git"
    }
  }
}
